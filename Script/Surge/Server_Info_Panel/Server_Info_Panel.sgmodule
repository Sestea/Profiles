#!name=Server Info Panel
#!desc=Display remote server information.

[Script]
Server_Info = type=generic,timeout=1000,script-path=https://raw.githubusercontent.com/Sestea/Profiles/master/Surge/Script/Server_Info.js,argument=server=http://[ADDRESS]:[PORT]

[Panel]
Server_Info_Panel = script-name=Server_Info,update-interval=600